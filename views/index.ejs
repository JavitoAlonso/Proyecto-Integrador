      <!-- Professions Cards -->
<section class="container-card">
  <% allprofessions.forEach(profesion => {%>
  <div class="card w-80 m-15 bg-6 ovf-hiden bg-2 b-shadow transition-ease">
    <figure>
        <img src="/images/<%= profesion.imagen %> " alt="gasista">
    </figure>
    <div class="contenido">
      <p class="descripcion">
        <%= profesion.descripcion %>  
      </p>
      <button type="button" class="btn btn-danger btn-floating"><%= profesion.profesion %> </button>
    </div>
  </div>
  <%  }); %> 
</section>

