<section class="mb-4 p-15 bg" id="contact">
  <h2 class="h1-responsive font-weight-bold text-center my-1 text-color">Contactanos!</h2>
  <div class="row container-fluid">

      <div class="col-md-9 mb-md-0 mb-5">
          
          <form class="form-outline mb-2">
            <label class="form-label text-color" for="form4Example1 ">Nombre</label>
            <input type="text" id="form4Example1" class="form-control form-control-sm" />
          </form>
        
        
          <form class="form-outline mb-2">
            <label class="form-label text-color" for="form4Example2">Email </label>
            <input type="email" id="form4Example2" class="form-control form-control-sm" />
          </form>
        
       
          <form class="form-outline mb-2">
            <label class="form-label text-color" for="form4Example3">Comentario</label>
            <textarea class="form-control" id="form4Example3" rows="4"></textarea>
          </form>
        
         
          <form class="form-check d-flex justify-content-center mb-4">
            <input class="form-check-input me-2" type="checkbox" value="" id="form4Example4" checked />
            <label class="form-check-label text-color" for="form4Example4">
              Enviarme copia del mensaje.
            </label>

          </form>
          <button type="submit" class="btn btn-danger btn-block mb-4">Enviar mensaje</button>
        </form>
          <div class="status"></div>
      </div>
      <div class="col-md-3 text-center contact-2">
          <ul class="list-unstyled mb-0">
              <li><i class="fas fa-map-marker-alt fa-2x color-primary"></i>
                  <p class="text-color">Av. Monroe 860, C1428 CABA</p>
              </li>
              <li><i class="fas fa-phone mt-4 fa-2x color-primary"></i>
